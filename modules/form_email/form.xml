<form action="{{app_abs}}/{{_d.q.0}}/send" method="post" class="form-email" id="form-{{Name}}">
<fieldset>
<legend>{{Title}}</legend>
<ul>
	<field><li style="{{style}}">{{field}}<label for="{{fid}}">{{name}}</label></li>
</field>
	<li class="clear"><label for="submit"></label><input type="submit" id="submit" value="Send" /></li>
</ul>
</fieldset>
</form>