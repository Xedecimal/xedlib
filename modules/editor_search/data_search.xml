<div class="data-search">

<results>
<div>
<result>
	<fieldset><legend>{{name}}</legend>
	<div class="actions">
		<result_button><a>{{text}}</a></result_button>
	</div>
	<ul>
	<result_field>
		<li><label>{{text}}</label>{{val}}</li>
	</result_field>
	</ul>
	</fieldset>
</result>
&laquo;
<page> <a href="{{url}}&amp;{{name}}_page={{num}}">{{num}}</a> </page>
&raquo;
</div>
</results>

<search>
<form id="data_search_{{name}}" action="{{app_abs}}/{{name}}/search" method="post">
<script type="text/javascript" src="editor_search/js.js"></script>
<fieldset>
<legend>Select fields to search</legend>
<table>
<searchfield><tr><td>
	<label><input type="checkbox" id="hider_{{id}}"
	name="{{name}}_search[{{fname}}]" class="hider" />{{text}}</label></td><td
	class="hidden ds_field" id="hidden_{{id}}">{{field}}</td></tr>
</searchfield>
</table>
</fieldset>
<input type="submit" value="Search" />
</form>
</search>

</div>